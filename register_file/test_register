#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x562c14877700 .scope module, "REGISTER3232_tb" "REGISTER3232_tb" 2 3;
 .timescale 0 0;
v0x562c1489f3e0_0 .var "Clk", 0 0;
v0x562c1489f4a0_0 .var "Clr", 0 0;
v0x562c1489f570_0 .var "D", 31 0;
v0x562c1489f670_0 .var "LE", 0 0;
v0x562c1489f740_0 .net "Q", 31 0, v0x562c1489f210_0;  1 drivers
S_0x562c14877890 .scope module, "reg0" "REGISTER32" 2 9, 3 1 0, S_0x562c14877700;
 .timescale 0 0;
    .port_info 0 /OUTPUT 32 "Q";
    .port_info 1 /INPUT 32 "D";
    .port_info 2 /INPUT 1 "LE";
    .port_info 3 /INPUT 1 "Clr";
    .port_info 4 /INPUT 1 "Clk";
v0x562c14888290_0 .net "Clk", 0 0, v0x562c1489f3e0_0;  1 drivers
v0x562c1489efa0_0 .net "Clr", 0 0, v0x562c1489f4a0_0;  1 drivers
v0x562c1489f060_0 .net "D", 31 0, v0x562c1489f570_0;  1 drivers
v0x562c1489f150_0 .net "LE", 0 0, v0x562c1489f670_0;  1 drivers
v0x562c1489f210_0 .var "Q", 31 0;
E_0x562c1484ef60 .event posedge, v0x562c14888290_0;
    .scope S_0x562c14877890;
T_0 ;
    %wait E_0x562c1484ef60;
    %load/vec4 v0x562c1489efa0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x562c1489f210_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x562c1489f150_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x562c1489f060_0;
    %assign/vec4 v0x562c1489f210_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x562c14877700;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x562c1489f3e0_0;
    %inv;
    %store/vec4 v0x562c1489f3e0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x562c14877700;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562c1489f3e0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562c1489f4a0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562c1489f670_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x562c1489f570_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562c1489f4a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562c1489f670_0, 0, 1;
    %pushi/vec4 2779096485, 0, 32;
    %store/vec4 v0x562c1489f570_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 28 "$display", "Test 1 - Clear: Q = %h (Expected: 00000000)", v0x562c1489f740_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562c1489f4a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562c1489f670_0, 0, 1;
    %pushi/vec4 3735928559, 0, 32;
    %store/vec4 v0x562c1489f570_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 33 "$display", "Test 2 - Load: Q = %h (Expected: DEADBEEF)", v0x562c1489f740_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562c1489f670_0, 0, 1;
    %pushi/vec4 3405691582, 0, 32;
    %store/vec4 v0x562c1489f570_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 38 "$display", "Test 3 - Hold Value: Q = %h (Expected: DEADBEEF)", v0x562c1489f740_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562c1489f4a0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 43 "$display", "Test 4 - Clear Again: Q = %h (Expected: 00000000)", v0x562c1489f740_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562c1489f4a0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562c1489f670_0, 0, 1;
    %pushi/vec4 305419896, 0, 32;
    %store/vec4 v0x562c1489f570_0, 0, 32;
    %delay 10, 0;
    %vpi_call 2 48 "$display", "Test 5 - Load New: Q = %h (Expected: 12345678)", v0x562c1489f740_0 {0 0 0};
    %vpi_call 2 49 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
